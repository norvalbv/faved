{
  "name": "@faved/data-store",
  "module": "index.ts",
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.30.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "migrate:generate": "bunx drizzle-kit generate --dialect sqlite --schema './schema/**/*.ts'",
    "migrate": "bun run utils/initDb.ts",
    "db:setup": "bun run migrate:generate && bun run migrate",
    "seed": "bun run ./utils/seed.ts",
    "check": "bun run utils/checkDb.ts",
    "clear-messages": "bun run utils/clearMessages.ts",
    "clear-financial-profiles": "bun run utils/clearFinancialProfiles.ts"
  },
  "type": "module",
  "dependencies": {
    "drizzle-orm": "0.38.2"
  }
}